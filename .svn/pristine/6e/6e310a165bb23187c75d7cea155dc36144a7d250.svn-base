package com.thp.structureaddress.model.gettransfomerinfo;

/**
 * 经纬度获取变压器ID
 * @author zhugb
 *
 */
public class TransformerBasicInfo {
	private int objectId =-1;
	private long dev_id = -1;
	private long gadget_id =-1;
	private double x = 0.0;
	private double y = 0.0;
	private String transformerId = null;
	private double distance = 0.0;
	private String basicInfoJsonString=null;
	
	/**
	 * 获取距离
	 * @return
	 */
	public double getDistance() {
		return distance;
	}
	/**
	 * 设定距离
	 * @param distance
	 */
	public void setDistance(double distance) {
		this.distance = distance;
	}
	/**
	 * 获取objectid
	 * @return
	 */
	public int getObjectId() {
		return objectId;
	}
	/**
	 * 设定objectid
	 * @param objectId
	 */
	public void setObjectId(int objectId) {
		this.objectId = objectId;
	}
	/**
	 * 获取变压器id
	 * @return
	 */
	public String getTransformerId() {
		return transformerId;
	}
	/**
	 * 设定变压器id
	 * @param transformerId
	 */
	public void setTransformerId(String transformerId) {
		this.transformerId = transformerId;
	}
	/**
	 * 获取设备id
	 * @return
	 */
	public long getDev_id() {
		return dev_id;
	}
	/**
	 * 设定设备id
	 * @param dev_id
	 */
	public void setDev_id(long dev_id) {
		this.dev_id = dev_id;
	}
	/**
	 * Gadget_id
	 * @return
	 */
	public long getGadget_id() {
		return gadget_id;
	}
	/**
	 * Gadget_id
	 * @param gadget_id
	 */
	public void setGadget_id(long gadget_id) {
		this.gadget_id = gadget_id;
	}
	/**
	 * 经度
	 * @return
	 */
	public double getX() {
		return x;
	}
	/**
	 * 经度
	 * @param x
	 */
	public void setX(double x) {
		this.x = x;
	}
	/**
	 * 纬度
	 * @return
	 */
	public double getY() {
		return y;
	}
	/**
	 * 纬度
	 * @param y
	 */
	public void setY(double y) {
		this.y = y;
	}
	
	
	
	public String getBasicInfoJsonString() {
		return basicInfoJsonString;
	}
	public void setBasicInfoJsonString(String basicInfoJsonString) {
		this.basicInfoJsonString = basicInfoJsonString;
	}
	/**
	 * 销毁
	 */
	public void Disponse()
	{	
		this.transformerId=null;	
	}	
	
}